<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.capps.md_udemy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MD_Udemy">
        <activity
            android:name=".StaticActivity"
            android:label="@string/title_activity_static"
            android:theme="@style/Theme.MD_Udemy.NoActionBar" />
        <activity
            android:name=".ScrollActivity"
            android:label="@string/title_activity_scroll"
            android:theme="@style/Theme.MD_Udemy.NoActionBar" />
        <activity android:name=".MainActivity" />
        <activity android:name=".MainMD">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>